{
  "manifest_version": 2,
  "name": "Youtube Resumer",
  "author": "Sebastien Beaury",
  "version": "1.0.0",
  "description": "Resume your videos on Youtube.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["tabs"],
  "content_security_policy": "script-src 'self' 'sha256-bYa5+qTPCwiUjcorwLU5xL8hkt2cfxlYzC1xEaFW6VA='; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/static/js/content.js"]
    }
  ]
}
